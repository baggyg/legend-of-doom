// Replace menu sounds
menu/cursor			ZRUPEE		// Move cursor up/down
menu/choose			BOMBDROP	// Choose a menu item
menu/activate		ZSHWITEM	// Activate a new menu
menu/backup			ARROBOOM	// Backup to previous menu
menu/prompt			ARROBOOM	// Activate a prompt "menu"
menu/change			ARROBOOM	// Select new value for option

menu/invalid		BOMBBLOW	// Menu not available
menu/dismiss		BOMBBLOW	// Dismiss a prompt message
menu/clear		    BOMBBLOW	// Close top menu

$random menu/quit1 { misc/gibbed zelda/boss1 zelda/boss2 zelda/boss3 }
$random menu/quit2 { misc/gibbed zelda/boss1 zelda/boss2 zelda/boss3 }

